<?xml version="1.0" encoding="utf-8"?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <service id="sulu_webspace.webspace_collection" class="Sulu\Bundle\WebspaceBundle\Model\WebspaceCollection">
            <argument type="collection" />
        </service>
        <service id="sulu_webspace.webspace_listener" class="Sulu\Bundle\WebspaceBundle\EventListener\WebspaceListener">
            <argument type="service" id="sulu_webspace.webspace_collection" />
            <tag name="kernel.event_subscriber" />
        </service>
    </services>
</container>
